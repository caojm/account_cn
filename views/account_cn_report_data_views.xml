<?xml version="1.0"?>
<odoo>
  <record id="account_cn_report_data_action" model="ir.actions.act_window">
    <field name="name">Report Data</field>
    <field name="res_model">account.cn.report.data</field>
    <field name="view_mode">tree,form</field>
  </record>
  <record id="account_cn_report_data_view_tree" model="ir.ui.view">
    <field name="name">Report Data Tree</field>
    <field name="model">account.cn.report.data</field>
    <field name="arch" type="xml">
      <tree editable="top">
        <field name="report_folder_id"/>
        <field name="report_item_id"/>
        <field name="report_model_id"/>
        <field name="comparison"/>
        <field name="input"/>
        <field name="filter_sum"/>
        <field name="aggregator_sum"/>
        <field name="final_data"/>
      </tree>
    </field>
  </record>
  <record id="account_cn_report_data_view_form" model="ir.ui.view">
    <field name="name">Report Data Form</field>
    <field name="model">account.cn.report.data</field>
    <field name="arch" type="xml">
      <form string="Report Data">
        <sheet>
          <group>
            <field name="report_folder_id"/>
            <field name="report_item_id"/>
          </group>
        </sheet>
      </form>
    </field>
  </record>
</odoo>
