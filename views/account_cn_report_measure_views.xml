<?xml version="1.0"?>
<odoo>
  <record id="account_cn_report_measure_action" model="ir.actions.act_window">
    <field name="name">Report Measure</field>
    <field name="res_model">account.cn.report.measure</field>
    <field name="view_mode">tree,form</field>
  </record>
  <record id="account_cn_report_measure_view_tree" model="ir.ui.view">
    <field name="name">Report Measure Tree</field>
    <field name="model">account.cn.report.measure</field>
    <field name="arch" type="xml">
      <tree>
        <field name="code"/>
        <field name="name"/>
      </tree>
    </field>
  </record>
  <record id="account_cn_report_measure_view_form" model="ir.ui.view">
    <field name="name">Report Measure Form</field>
    <field name="model">account.cn.report.measure</field>
    <field name="arch" type="xml">
      <form string="Report Measure">
        <header>
        </header>
        <sheet>
          <group>
            <group>
              <field name="code" />
            </group>
            <group>
              <field name="name" />
            </group>
          </group>
        </sheet>
      </form>
    </field>
  </record>
</odoo>
