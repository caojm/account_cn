<?xml version="1.0"?>
<odoo>
  <record id="account_cn_report_model_action" model="ir.actions.act_window">
    <field name="name">Report Model</field>
    <field name="res_model">account.cn.report.model</field>
    <field name="view_mode">tree,form</field>
  </record>
  <record id="account_cn_report_model_view_tree" model="ir.ui.view">
    <field name="name">Report Model Tree</field>
    <field name="model">account.cn.report.model</field>
    <field name="arch" type="xml">
      <tree>
        <field name="code"/>
        <field name="name"/>
      </tree>
    </field>
  </record>
  <record id="account_cn_report_model_view_form" model="ir.ui.view">
    <field name="name">Report Model Form</field>
    <field name="model">account.cn.report.model</field>
    <field name="arch" type="xml">
      <form string="Report Model">
        <header>
        </header>
        <sheet>
          <group>
            <group>
              <field name="code" />
              <field name="name" />
            </group>
            <group>
              <field name="company_id" />
            </group>
            <notebook>
              <page string="Items">
                <field name="item_ids" />
              </page>
            </notebook>
          </group>
        </sheet>
      </form>
    </field>
  </record>
</odoo>
